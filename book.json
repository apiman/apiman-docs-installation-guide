{
    "gitbook": "3.x.x",
    "structure": {
        "readme": "README.adoc"
    },
    "plugins": [
        "anchors",
        "theme-apiman"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "pluginsConfig": {
            "theme-apiman": {
                "split": false,
                "languages": [
                    {
                        "lang": "Vertx",
                        "name": "Vert.x",
                        "default": true
                    },
                    {
                        "lang": "Servlet",
                        "name": "Servlet"
                    }
                ]
            }
        }
    },
    "variables": {
        "apiman": {
            "version": {
                "release": "1.2.9.Final",
                "snapshot": "1.2.10-SNAPSHOT"
            }
        },
        "wildfly10": {
            "version": "10.1.0.Final"
        },
        "wildfly9": {
            "version": "9.0.2.Final"
        }
    }
}
